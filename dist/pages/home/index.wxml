        <wxc-popup class="popup1" contAlign="center" _pid="{{_pid}}">
            <view style="background-color: #eeeeee;height: 140px;width:140px">
                还好还好
                <wxc-icon style="color: red" _pid="{{_pid}}"></wxc-icon>
            </view>
